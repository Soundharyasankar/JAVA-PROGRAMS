public class program4 {
   public program4() {
   }

   public static void main(String[] var0) {
      System.out.println(isPrime(18));
      System.out.println(isPrime(7));
   }

   public static boolean isPrime(int var0) {
      for(int var1 = 2; var1 * var1 <= var0; ++var1) {
         if (var0 % var1 == 0) {
            return false;
         }
      }

      return true;
   }
}
